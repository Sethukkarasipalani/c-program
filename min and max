#include<s
#include <stdio.h>

int main() {
    int n, max, min;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    printf("Enter %d numbers: ", n);
    
    scanf("%d", &max);
    min = max;
    n--;
    
     while (n--) {
        int num;
        scanf("%d", &num);
        if (num > max)
            max = num;
        if (num < min)
            min = num;
    }
    
    printf("Range: %d\n", max - min);
    return 0;
}
