#include <stdio.h>
int main()
{
    int num1, num2, temp, gcd, lcm;
    printf("Enter first number: ");
    scanf("%d", &num1);
    printf("Enter second number: ");
    scanf("%d", &num2);
    temp = num2;
    while (num2 != 0) {
        int remainder = num1 % num2;
        num1 = num2;
        num2 = remainder;
    }
    gcd = num1;
    lcm = (temp * (num1 * num2)) / gcd;
    printf("GCD: %d\n", gcd);
    printf("LCM: %d\n", lcm);
    return 0;
}

